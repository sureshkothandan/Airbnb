datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt  DateTime @default(now())
  firstName  String?
  id         String   @id @default(cuid())
  lastName   String?
  listings   Listing? @relation(fields: [listingsId], references: [id])
  listingsId String?
  password   String
  roles      Json
  updatedAt  DateTime @updatedAt
  username   String   @unique
}

model Listing {
  createdAt        DateTime @default(now())
  description      String?
  id               String   @id @default(cuid())
  listingCreatedBy User[]
  locationData     Json?
  locationType     String?
  mapData          Json?
  photos           Json?
  placeSpace       Json?
  placeType        String?
  price            Int?
  title            String
  updatedAt        DateTime @updatedAt
}
